<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Device specific doze package -->
    <string name="config_customDozePackage">org.lineageos.settings/org.lineageos.settings.doze.DozeSettingsActivity</string>

    <!-- Use only if devices have variable intensity support for haptics-->
    <bool name="config_vibration_supports_multiple_intensities">true</bool>

    <!-- Whether device has screen off FOD support -->
    <bool name="config_supportScreenOffFod">true</bool>

    <!-- Whether to show a preference item for mobile plan -->
    <bool name="config_show_mobile_plan">false</bool> 

    <!-- Whether to show a preference item for regulatory information in About phone -->
    <bool name="config_show_regulatory_info">true</bool>
</resources>
